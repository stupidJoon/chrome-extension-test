{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "options_page": "./options.html",
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*"
    ],
    "manifest_version": 2
}